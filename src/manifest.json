{
    "manifest_version":2,

    "name":"test extension",
    "version":"0.0.0.3",
    "description":"test extension",
    "permissions":[
        "activeTab",
        "tabs",
        "storage",
        "background",
        "sessions",
        "http://www.softomate.net/",
        "http://*/",
        "https://*/"
    ],
    "browser_action": {
        "default_title": "test extension",
        "default_popup": "index.html",
        "default_icon": "img/donation.png"
    },
    "background": {
        "scripts": ["js/jquery-3.3.1.min.js", "js/background.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": ["img/donation.png"]
}
